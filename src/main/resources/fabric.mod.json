{
	"schemaVersion": 1,
	"id": "oxide",
	"version": "${version}",
	"name": "Oxide",
	"description": "Minecraft world generation reimplemented and optimized in Rust.",
	"authors": [
		"VioletSpace"
	],
	"contact": {
		"homepage": "https://github.com/VioletSpace/oxide",
		"sources": "https://github.com/VioletSpace/oxide"
	},
	"license": "CC0-1.0",
	"icon": "assets/oxide/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.oxide_worldgen.Oxide"
		],
		"client": [
			"com.oxide_worldgen.OxideClient"
		]
	},
	"mixins": [
		"oxide.mixins.json",
		{
			"config": "oxide.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.13",
		"minecraft": "~1.21.5",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}